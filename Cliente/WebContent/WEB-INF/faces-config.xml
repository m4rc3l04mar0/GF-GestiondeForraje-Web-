<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <navigation-rule>
  <from-view-id>/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>nuevoUsuario</from-outcome>
   <to-view-id>/Usuarios/NuevoUsuario.xhtml</to-view-id>
   <redirect include-view-params="true"/>
  </navigation-case>
  <navigation-case>
   <from-outcome>listarUsuarios</from-outcome>
   <to-view-id>/Usuarios/ListarUsuarios.xhtml</to-view-id>
   <redirect include-view-params="true"/>
  </navigation-case>
   <navigation-case>
   <from-outcome>nuevoPredio</from-outcome>
   <to-view-id>/Usuarios/Predios/NuevoPredio.xhtml</to-view-id>
   <redirect include-view-params="true"/>
  </navigation-case>
  <navigation-case>
   <from-outcome>listarPredios</from-outcome>
   <to-view-id>/Usuarios/Predios/ListarPredios.xhtml</to-view-id>
   <redirect include-view-params="true"/>
  </navigation-case>
  <navigation-case>
   <from-outcome>nuevoPotrero</from-outcome>
   <to-view-id>/Usuarios/Potreros/NuevoPotrero.xhtml</to-view-id>
   <redirect include-view-params="true"/>
  </navigation-case>
  <navigation-case>
   <from-outcome>listarPotreros</from-outcome>
   <to-view-id>/Usuarios/Potreros/ListarPotreros.xhtml</to-view-id>
   <redirect include-view-params="true"/>
  </navigation-case>
  <navigation-case>
   <from-outcome>nuevaRecorrida</from-outcome>
   <to-view-id>/Usuarios/Recorridas/NuevoRecorrida.xhtml</to-view-id>
   <redirect include-view-params="true"/>
  </navigation-case>
  <navigation-case>
   <from-outcome>listarRecorridas</from-outcome>
   <to-view-id>/Usuarios/Recorridas/ListarRecorridas.xhtml</to-view-id>
   <redirect include-view-params="true"/>
  </navigation-case>
  <navigation-case>
   <from-outcome>salir</from-outcome>
   <to-view-id>/Usuarios/login.xhtml</to-view-id>
   <redirect include-view-params="true"/>
  </navigation-case>
 
 </navigation-rule>
 <application/>
</faces-config>
